    "@mui/material": "^6.4.2",
    "@mui/styled-engine-sc": "^6.4.2",
    "@react-oauth/google": "^0.12.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/vite": "^4.0.3",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.3",
    "dompurify": "^3.1.6",
    "dotenv": "^16.4.7",
    "gsap": "^3.13.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
